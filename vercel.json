{
    "version": 2,
    "builds": [
        {"src": "scrapperAPI.py", "use": "@vercel/python"}
    ],
    "routes": [
    {
      "src": "/(.*)",
      "dest": "./scrapperAPI.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
